{
  "functions": {
    "source": ".output/server"
  },
  "hosting": [
    {
      "site": "fireux-mvp",
      "public": ".output/public",
      "cleanUrls": true,
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        { "source": "**", "function": "server" }
      ]
    }
  ]
}